{
    "name": "ycy",
    "displayName": "ycy",
    "description": "",
    "version": "0.0.1",
    "publisher": "formulahendry",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:ycy.showReminderView",
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "ycy.showReminderView",
                "title": "打开提醒页面",
                "category": "ycy"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
